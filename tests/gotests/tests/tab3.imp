[ tab3.imp - duuuuuze tablice 

        Przykladowe dane testowe:
        1. 0, 9 OK
        2. 7777, 9 OK
        3. 654321, 3 OK
]
DECLARE
  tab(0:1325476980), a, b, c, d, n 
BEGIN
  READ a;
  tab(a) := 9;

  n := 1325476980;
  tab(n) := tab(a);
  
  b :=  111111111;
  b := b * tab(a);
  
  c := 90909090;
  READ d;
  
  tab(a) := b - c;
  tab(n) := tab(a) / d;
  WRITE tab(a);
  WRITE tab(n);
END 
