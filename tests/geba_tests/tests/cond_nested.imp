[ cond_nested.imp - instrukcje warunkowe 

        Przykladowe dane testowe:
        1. 12, 20, 7 OK
        2. 7, 5, 7
]
DECLARE
	a, aa, b, c, d, e, f, g
BEGIN
	READ a;
	READ aa;
	READ b;
	g := 1111111111;
	c := a + 10;
	d := b - 5;
	e := 1;
	f := d + e;

	WRITE g;
	WRITE a;
	WRITE aa;
	WRITE b;
	WRITE c; [ c = a + 10 ]
	WRITE d; [ d = b - 5 ]
	WRITE e; [ e = 1 ]
	WRITE f; [ f = d + e ]
	WRITE g;

	IF a > b THEN	[ if a > b then e = a + c ]
		e := a + c;
		IF aa < a THEN	[ if a > aa then f = e * aa ]
			f := e * aa;
		ELSE
			f := e * a;	[ if a <= aa then f = e * a ]
		ENDIF
	ELSE		[ if a <= b then e = b + c ]
		e := b + c;
		IF aa > a THEN [ if aa > a then f = e / a ]
			f := e / a;
		ELSE			[ if aa <= a then f = e / aa ]
			f := e / aa;
		ENDIF
	ENDIF

	WRITE a;
	WRITE aa;
	WRITE b;
	WRITE c;
	WRITE d;
	WRITE e;
	WRITE f;
END

